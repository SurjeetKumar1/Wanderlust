<% layout("/layouts/boilerplate.ejs") -%>

<div class="container text-center mt-5">
  <h2 class="text-danger mb-3">No Listings Found</h2>
  <p class="lead">
    Sorry, we couldn't find any listings 
    <% if (search) { %> matching "<strong><%= search %></strong>"<% } %>
    <% if (category) { %> in category "<strong><%= category %></strong>"<% } %>.
  </p>
  <a href="/listings" class="btn btn-primary mt-3">Back to All Listings</a>
</div>